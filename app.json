{
  "pages": [
    "pages/home/index",
    "pages/test-simple/index",
    "pages/frequency-match/index",
    "pages/messages/index",
    "pages/publish/index",
    "pages/profile/index"
  ],
  "tabBar": {
    "custom": true,
    "color": "#999999",
    "selectedColor": "#FF6B8B",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/home/index",
        "text": "首页",
        "iconPath": "assets/icons/home.png",
        "selectedIconPath": "assets/icons/home-active.png"
      },
      {
        "pagePath": "pages/frequency-match/index",
        "text": "相遇",
        "iconPath": "assets/icons/heart.png",
        "selectedIconPath": "assets/icons/heart-active.png"
      },
      {
        "pagePath": "pages/messages/index",
        "text": "私信",
        "iconPath": "assets/icons/message.png",
        "selectedIconPath": "assets/icons/message-active.png"
      },
      {
        "pagePath": "pages/publish/index",
        "text": "发布",
        "iconPath": "assets/icons/plus.png",
        "selectedIconPath": "assets/icons/plus-active.png"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "我的",
        "iconPath": "assets/icons/user.png",
        "selectedIconPath": "assets/icons/user-active.png"
      }
    ]
  },
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTitleText": "PINYU CLUB",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#f8f9fa",
    "navigationStyle": "custom",
    "enablePullDownRefresh": false,
    "onReachBottomDistance": 50,
    "pageOrientation": "portrait",
    "restartStrategy": "homePage"
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "usingComponents": {
    "navbar": "/components/navbar/index",
    "top-navbar": "/components/top-navbar/index",
    "bottom-navbar": "/components/bottom-navbar/index",
    "hero-banner": "/components/hero-banner/index",
    "quick-actions": "/components/quick-actions/index",
    "search-input": "/components/search-input/index",
    "event-card": "/components/event-card/index",
    "banner-carousel": "/components/banner-carousel/index",
    "member-card": "/components/member-card/index"
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  }
}