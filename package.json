    "lint": "eslint *.js --no-eslintrc --env node --env es6 --parser-options ecmaVersion:2020 --rule 'no-unused-vars:error' --rule 'no-undef:error' --rule 'semi:error' --rule 'quotes:[error,single]' --rule 'indent:[error,2]' --rule 'no-trailing-spaces:error' --rule 'space-before-function-paren:[error,always]' --rule 'no-extra-semi:error' --rule 'padded-blocks:[error,never]' --rule 'no-new:error' --rule 'eol-last:error'",
